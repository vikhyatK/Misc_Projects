<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN 2.0//EN"
"http://www.springframework.org/dtd/spring-beans-2.0.dtd">
<beans>
	<bean id="e1" class="beans.Engine" scope="prototype">
	    <property name="modelYear" value="2013"/>
	</bean>
	<bean id="e2" class="beans.Engine" scope="prototype">
	    <property name="modelYear" value="2014"/>
	</bean>
	<bean id="e3" class="beans.Engine" scope="prototype">
	    <property name="modelYear" value="2015"/>
	</bean>
	
	<bean id="c" class="beans.Car" scope="prototype">
	    <property name="carName">
	        <list>
	            <value>Audi</value>
	            <value>Honda</value>
	            <value>Benz</value>
	        </list>
	    </property>
	    
	    <property name="engine">
	        <list>
	            <ref bean="e1"/>
	            <ref bean="e2"/>
	            <ref bean="e3"/>
	        </list>
	    </property>
	</bean>
</beans>