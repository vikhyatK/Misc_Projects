<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.mvc.bean.Vets" 
        table="vets" >
 
        <id name="vet_id" type="int">
            <column name="vet_id" />
            <generator class="native" />
        </id>
        <property name="vet_fname" type="string">
            <column name="vet_fname" />
        </property>
        <property name="vet_lname" type="string">
            <column name="vet_lname" />
        </property>
        <property name="speciality" type="string">
            <column name="speciality" />
        </property>
        <property name="phone" type="int">
            <column name="phone" />
        </property>
       
        
    </class>
</hibernate-mapping>